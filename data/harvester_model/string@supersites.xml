<HarvesterModel>
  <id>supersites</id>
  <catalogType>opensearch</catalogType>
  <source>
    <url>http://spirit.cnes.fr/supersites/ws/search.php?q={searchTerms}&amp;bbox={geo:box?}&amp;format=json&amp;startDate={time:start?}&amp;completionDate={time:stop?}&amp;modified={time:start?}</url>
  </source>
  <description>Harvesting of Supersites</description>
  <harvesterClassName>fr.cnes.sitools.metacatalogue.opensearch.OpensearchHarvester</harvesterClassName>
  <indexerConf>
    <url>http://localhost:8983/solr/supersites</url>
  </indexerConf>
  <attributes/>
  <automaticMerge>false</automaticMerge>
  <publicServices>true</publicServices>
  <status>INACTIVE</status>
</HarvesterModel>